{
  "name": "travels-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "Travel Sales Management API with Express and Prisma",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "dev:debug": "nodemon --inspect src/server.js",
    "prisma:generate": "cross-env DATABASE_URL=\"mongodb+srv://flyaid-management:flyaid-management@cluster0.kqlaj.mongodb.net/travelsDB?retryWrites=true&w=majority&appName=Cluster0\" prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "express",
    "prisma",
    "travel",
    "sales",
    "api",
    "rest"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "prisma": "^6.19.0",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}